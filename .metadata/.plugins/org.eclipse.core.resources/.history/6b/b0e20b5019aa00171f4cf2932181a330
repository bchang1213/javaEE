package com.brianchang.web.models;

public class Dog extends Animal implements Pet{

	@Override
	public String showAffection() {
		int w = getWeight();
		if(weight < 30) {
			return name + " hopped into your lap and cuddled you!"
		}
		if(weight >= 30) {
			return name + " curled up next to you!"
		}
	}

}
